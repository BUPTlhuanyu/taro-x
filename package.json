{
  "name": "taro-x",
  "version": "1.0.0",
  "private": false,
  "description": "taro组件库",
  "main": "dist/index.js",
  "main:h5": "dist/h5/index.js",
  "types": "./types/index.d.ts",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "build:component": "cross-env TARO_BUILD_TYPE=component taro build --ui",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "cross-env NODE_ENV=test && jest --coverage",
    "test:u": "cross-env NODE_ENV=test && jest --coverage -u"
  },
  "files": [
    "dist",
    "types",
    "map/map.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/mini-runner": "^2.0.5",
    "@types/react": "^16.4.8",
    "bind-decorator": "^1.0.11",
    "classnames": "^2.2.6",
    "dayjs": "^1.7.7",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.1",
    "@babel/preset-env": "^7.9.0",
    "@tarojs/cli": "2.0.0",
    "@tarojs/components": "2.0.0",
    "@tarojs/plugin-babel": "2.0.0",
    "@tarojs/plugin-csso": "2.0.0",
    "@tarojs/plugin-sass": "2.0.0",
    "@tarojs/plugin-uglifyjs": "2.0.0",
    "@tarojs/router": "2.0.0",
    "@tarojs/taro": "2.0.0",
    "@tarojs/taro-alipay": "2.0.0",
    "@tarojs/taro-h5": "2.0.0",
    "@tarojs/taro-qq": "2.0.0",
    "@tarojs/taro-quickapp": "2.0.0",
    "@tarojs/taro-swan": "2.0.0",
    "@tarojs/taro-tt": "2.0.0",
    "@tarojs/taro-weapp": "2.0.0",
    "@tarojs/webpack-runner": "2.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-taroapi": "2.0.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-config-taro": "2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "2.0.0",
    "jest": "23.6.0",
    "nerv-devtools": "^1.5.6",
    "nerv-server": "^1.5.6",
    "nerv-test-utils": "^1.5.6",
    "nervjs": "^1.5.6",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.0.0",
    "stylelint-taro-rn": "2.0.0",
    "typescript": "^3.0.1"
  }
}
